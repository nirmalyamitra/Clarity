    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Protractor",
            "program": "${workspaceRoot}/node_modules/protractor/bin/protractor",
            "stopOnEntry": false,
            "args": [
                "${workspaceRoot}/config.js"
            ],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type" : "node",
            "request": "attach",
            "name" : "Attach to Process",
            "address": "localhost",
            "port": 5858,
            "outFiles": []

        }
    ]
}